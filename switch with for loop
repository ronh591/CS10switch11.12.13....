#include <iostream>
#include <string>
using namespace std;
int main() {
cout << "guess what 2 + 2 is  ";
int myInt;
cin >> myInt;
cout << "*****\n&&&&&&\n#####" <<endl;
switch(myInt)
{
case 5:
cout << "too high" << endl;
for (int i=0; i < 5; i++) {
cout << "*****\n";}


break;
case 3:
cout << "too low" << endl;
break;
case 4:
cout << "exactly " << endl;
break;
default: 
  cout << "way off" << endl;
}
}
