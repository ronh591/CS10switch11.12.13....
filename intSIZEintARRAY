#include <iostream>
using namespace std;

void bubble_sort(int array[], int size) {
  for (int i = 0; i < size - 1; i++) {
    for (int j = 0; j < size - i -1; j++) {
      if (array[j] > array[j+1]) {
        int temp = array[j];
        array[j] = array[j+1];
        array[j+1] = temp;
        }
      }
    }

}
int main() {
  int array[] = {5, 3, 4, 1, 2, 33,345,122,111222,444,333333,352525,5435345,345454,435545435,34553646,45654656};
  int size = sizeof(array)/sizeof(array[0]);
  cout << size << endl;
  bubble_sort(array, size);
  for(int i = 0; i < size; i++) {
    cout << array[i] << " ";
  }
  cout << endl;

}
